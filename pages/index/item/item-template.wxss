.item-wrapper{
  padding:0 30rpx;
}
.item{
  padding:30rpx 0;
  display:flex;
  justify-content: space-between;
  border-bottom:1rpx solid #e5e5e5;
  font-size: 16px;
}
.item image{
  width: 130px;
  margin-right:20rpx;
}
.item-info{
  width:100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.item-info .time{
  text-align: right;
  font-size:12px;
  color:#d0d0d0;
}